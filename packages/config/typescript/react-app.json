{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable"],
    "module": "NodeNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "es5"
  }
}
